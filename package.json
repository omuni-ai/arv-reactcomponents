{
  "name": "groot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@svgr/webpack": "4.1.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.0",
    "body-parser": "^1.19.0",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "compression": "^1.7.4",
    "cross-env": "^5.2.0",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "express": "^4.17.3",
    "forever": "^1.0.0",
    "fs": "^0.0.1-security",
    "fs-extra": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "intersection-observer": "^0.7.0",
    "is-wsl": "^1.1.0",
    "jest": "24.7.1",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.3.0",
    "mini-css-extract-plugin": "0.5.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-decorators": "^1.2.2",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-proptypes": "^1.0.0",
    "react-syntax-highlighter": "^8.1.0",
    "resolve": "1.10.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "7.1.0",
    "semver": "6.0.0",
    "source-map-support": "^0.5.12",
    "style-loader": "0.23.1",
    "stylelint-declaration-strict-value": "^1.1.3",
    "stylelint-scss": "^3.10.0",
    "ts-pnp": "1.1.2",
    "workbox-webpack-plugin": "4.2.0"
  },
  "scripts": {
    "lint": "eslint src --ext .jsx,.js",
    "lintfix": "yarn eslint --fix .",
    "start": "node scripts/start.js",
    "start:prod": "NODE_ENV=production npm run build && node scripts/start.js",
    "start:dev": "NODE_ENV=development npm run build && node scripts/start.js",
    "build": "node scripts/build.js",
    "build:react": "node scripts/build.js",
    "build:bundle": "webpack --config webpack.config.js",
    "test": "node scripts/test.js",
    "start-prod": "cross-env NODE_ENV=production && node ./src/server.js",
    "initpackage": "cd bundle && rm -rf components",
    "bundle": "npm run initpackage && rollup -c config/rollup.config.js",
    "publishpackage": "npm run bundle && cd bundle && npm publish"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "css-loader": "2.1.1",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-prettier": "^3.1.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "normalize.css": "^7.0.0",
    "prettier": "^1.18.2",
    "rollup": "^1.16.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "stylelint": "^10.1.0",
    "stylelint-order": "^3.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "terser-webpack-plugin": "1.2.3",
    "url-loader": "1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
